// Given an integer num, repeatedly add all its digits until the result has only one digit, and return it.

int addDigits(int num) {
    int sum_of_digits= 0 , replica = num;
 if(num < 10)   
 {
    return num ;

 }
 else
 {
    come_here :
    while(replica)
    {
        sum_of_digits += replica % 10;
        replica /= 10;
    }
    if(sum_of_digits >= 10)
    {
        replica = sum_of_digits ;
        sum_of_digits = 0;
        goto come_here;
    }
    else
    {
        return sum_of_digits;
    }
 }
}
