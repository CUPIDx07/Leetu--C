/*Given an array nums of integers, return how many of them contain an even number of digits*/

int findNumbers(int* nums, int numsSize) {
    int digit_count , count = 0 ;
    for(int i = 0 ; i< numsSize ; i++ )
    {
        digit_count = 0;
        while(nums[i])
        {
            digit_count++;
            nums[i]= nums[i] / 10;
        }
        if(digit_count % 2 == 0)
        {
            count++;
        }
    }
    return count;
    
}
